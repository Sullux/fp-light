{
  "name": "@sullux/fp-light",
  "version": "1.0.0-beta.0",
  "engines": {
    "node": ">=8.*"
  },
  "description": "A lightweight library to support functional programming in javascript.",
  "keywords": [
    "fp",
    "functional"
  ],
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --config webpack.config.js",
    "package": "yarn build && node ./package.js && cp README.md dist/",
    "unit": "./node_modules/.bin/mocha ./lib/**/*.spec.js",
    "lint": "./node_modules/.bin/eslint *.js ./lib/**/*.js",
    "fix": "./node_modules/.bin/eslint *.js ./lib/**/*.js --fix",
    "test": "yarn lint && yarn unit",
    "deploy": "yarn unit && yarn package && cd dist && npm publish --access public"
  },
  "author": "Charles Sullivan <sullux@gmail.com> (https://github.com/Sullux)",
  "license": "MIT",
  "repository": "sullux/fp-light",
  "homepage": "https://github.com/Sullux/fp-light/blob/master/README.md",
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "eslint": "^5.14.1",
    "esprima": "^4.0.1",
    "js-yaml": "^3.13.1",
    "vlq": "^1.0.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  }
}
